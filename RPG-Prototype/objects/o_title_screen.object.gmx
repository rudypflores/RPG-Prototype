            <string>/// Determine position of selection
//Controls
down = keyboard_check_pressed(vk_down);
up = keyboard_check_pressed(vk_up);
select = keyboard_check(ord('Z'));

//handle selection
if(select) {
    switch(selected) {
        case START:
            room_goto(r_house_dyru);
            break;
        case LOAD:
            //load save state
            break;
        case EXIT:
            game_end();
            break;
    }
}

//handle movement
if(up) {
    //go back one option
    selected--;
} else if(down) {
    selected++;
}

//handle cases
if(selected &lt; 0)
    selected = len-1; //becomes last option
else if(selected == len)
    selected = 0; //becomes first option

//reassign position based on input   
pos = options[selected];
o_selection.y = pos;
</string>
